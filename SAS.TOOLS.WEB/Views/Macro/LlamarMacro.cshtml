@model IEnumerable<SAS.TOOLS.DTO.SAS_MACRO_DTO>

@{
    ViewBag.Title = "LlamarMacro";
}

<h2>Llamar Macro</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID_MACRO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MACRO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ORIGEN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STATEMENT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ORDEN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PRIORIDAD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.REQUERIDO)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ID_MACRO)
            
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MACRO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ORIGEN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.STATEMENT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ORDEN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRIORIDAD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.REQUERIDO)
        </td>
        <td>
        
        </td>
    </tr>
  
}

</table>


<form method="post">
<p><textarea cols="40" rows="8" name="Contenido" autofocus="autofocus" readonly>
    @ViewBag.DaMacro
</textarea>
</p>
    

</form>


